    CONSTRAINT PK_sys_otros_ingresos PRIMARY KEY (id)
)
 ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish2_ci;

# ---------------------------------------------------------------------- #
