    CONSTRAINT PK_sys_retenciones PRIMARY KEY (id)
)
 ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish2_ci;

# ---------------------------------------------------------------------- #
# Add table "sys_roles"                                                  #
